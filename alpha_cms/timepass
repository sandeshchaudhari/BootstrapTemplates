<div class="c_user-notification"
user-notifications-list
view-mode="instructor,student"
unread-count="1">
<div class="dropdown dropdown--notification">
<div class="dropdown__toggle">
<a class="dropdown__main-link"
click-on-mobile-and-key-press
aria-expanded="false"
role="button"
dropdown-init="initializeNotifications(activeTab)"
href="/user/view-notifications/"
header-tracking="notifications.main">
<div class="pos-r">
<i class="udi udi-bell dropdown__main-icon"></i>
<span class="sr-only">Notifications</span>

<b class="badge ng-cloak"
ng-if="tabs.instructor.counts.unreadCount + tabs.student.counts.unreadCount > 0"
aria-label="{{ ninterpolate('%s new notification', '%s new notifications', tabs.instructor.counts.unreadCount + tabs.student.counts.unreadCount) }}">
{{ tabs.instructor.counts.unreadCount + tabs.student.counts.unreadCount }}
</b>
<b class="badge ng-cloak"
ng-show="(tabs.instructor.counts.total + tabs.student.counts.total == 0) && unreadCount > 0"
aria-label="{{ ninterpolate('%s new notification', '%s new notifications', unreadCount) }}">
{{ unreadCount }}
</b>

</div>
</a>
</div>
<div class="dropdown__menu" role="dialog" aria-label="Notifications">
<div ng-include="notificationsListTemplate" onload="loadingNotificationTemplate = false"></div>
<div class="p10 fx-c" ng-show="loadingNotifications(activeTab) || loadingNotificationTemplate">
